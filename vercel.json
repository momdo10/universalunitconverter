{
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/sv",
      "destination": "/sv/index.html",
      "permanent": true
    },
    {
      "source": "/sv/index.html",
      "destination": "/sv",
      "permanent": true
    },
    {
      "source": "/:path((?!sv).*)/",
      "has": [
        {
          "type": "header",
          "key": "Accept-Language",
          "value": "(?i).*sv.*"
        }
      ],
      "destination": "/sv",
      "statusCode": 307
    }
  ]
}

  
